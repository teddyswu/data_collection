<div class="row">
  <p></p>
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-heading">
        <i class="fa fa-bar-chart-o fa-fw"></i> All Customer Base
      </div>
      <!-- /.panel-heading -->
      <div class="panel-body">
        <div class="row">
          <div class="col-lg-12">
            <div class="table-responsive">
              <table class="table table-bordered table-hover table-striped">
                <thead>
                  <tr>
                    <th>#</th>
                    <th><%= t('rtmm.name')%></th>
                    <th><%= t('rtmm.enter')%></th>
                  </tr>
                </thead>
                <tbody>
                  <% @cate.each do |cate| %>
                    <tr>
                      <td><%= cate.id %></td>
                      <td><%= cate.name %></td>
                      <td><%= link_to "Go", rtmm_supplier_path(cate.id) %></td>
                    </tr>
                  <% end %>
                </tbody>
              </table>
            </div>
            <!-- /.table-responsive -->
          </div>
          <!-- /.col-lg-4 (nested) -->
        </div>
        <!-- /.row -->
      </div>
      <!-- /.panel-body -->
    </div>
    <!-- /.panel -->    
  </div>
</div>